<?xml version="1.0"?>
<launch>
    <include file="$(find fanuc_moveit_config)/launch/planning_context.launch">
        <arg name="load_robot_description" value="true" />
    </include>

    <include file="$(find fanuc_moveit_config)/launch/demo.launch"></include>

    <arg name="debugger_attached" default="false" />
    <node name="IKActionServer" type="IK_server_node" pkg="kinematics_action" output="screen" unless="$(arg debugger_attached)" />

    <node name="IKActionClient" type="IK_client_node" pkg="kinematics_action" output="screen"/>
    
    <node name="joint_state_publisher_gui" type="joint_state_publisher_gui" pkg="joint_state_publisher_gui" />

</launch>